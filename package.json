{
  "name": "sveltyper",
  "version": "1.0.0",
  "description": "A tool to generate declaration files for Svelte component.",
  "main": "dist/index.js",
  "bin": "dist/bin",
  "scripts": {
    "build": "rollup -c",
    "test": "uvu -r esm test -i fixtures -i cli",
    "test:update": "npm run build && node ./test/fixtures/fixtures.js",
    "prepublishOnly": "npm run build"
  },
  "svelte": "test.js",
  "keywords": [
    "svelte",
    "typescript",
    "declaration",
    "dts"
  ],
  "author": "TheComputerM",
  "license": "MIT",
  "homepage": "https://github.com/TheComputerM/sveltyper#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheComputerM/sveltyper.git"
  },
  "bugs": {
    "url": "https://github.com/TheComputerM/sveltyper/issues"
  },
  "dependencies": {
    "svelte-docster": "^1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "astray": "^1.1.1",
    "esm": "^3.2.25",
    "meriyah": "^4.1.5",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^3.37.0",
    "uvu": "^0.5.1"
  },
  "files": [
    "dist"
  ]
}
